#!/usr/bin/env bash
#bash script that gives you information about the school file. Using if and else.
i=school
if [ -e $i ]
then
echo "school file exists"
else
echo "school file does not exist"
fi
if [ -s $i ]
then
echo "school file is empty"
else
echo "school file is not empty"
fi
else[ -r $i ]
echo "school is a regular file"

